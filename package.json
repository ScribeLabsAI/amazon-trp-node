{
  "name": "@scribelabsai/amazon-trp",
  "version": "3.0.4",
  "description": "Amazon Textract Response Parser library for Node.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": "./dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build:lib": "tsc",
    "build:bin": "cd bin && tsc",
    "message": "git config commit.template .gitmessage",
    "check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--experimental-vm-modules' npx jest --injectGlobals false --logHeapUsage --silent false --passWithNoTests",
    "coverage": "npm run test -- --coverage"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScribeLabsAI/amazon-trp-node.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "aws-textract-parser",
    "typescript",
    "aws-textract",
    "aws"
  ],
  "author": "Emmanuel Hadoux <emmanuel@scribelabs.ai>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ScribeLabsAI/amazon-trp-node/issues"
  },
  "homepage": "https://github.com/ScribeLabsAI/amazon-trp-node#readme",
  "devDependencies": {
    "@aws-sdk/client-textract": "^3.293.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.1.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "zod": "^3.21.4",
    "commander": "^11.0.0"
  }
}